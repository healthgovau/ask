<div class="health-card health-card--shadow">
	<div class="wrapper">
		<div class="health-card__image health-card__image--full-width image--link">
			<a href="{{ url }}">
				{{ image }}
			</a>
		</div>
		<h3 class="health-card__title au-display-md">
			<a href="{{ url }}">{{ title }}</a>
		</h3>
		{% block content %}
			{{ content }}
		{% endblock %}
	</div>
</div>
